{
  "tasks": {
    "start": "deno run --watch -A --env src/app.tsx",
    "debug": "deno run --inspect -A --env src/app.tsx"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  },
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "noImplicitAny": false
  }
}
